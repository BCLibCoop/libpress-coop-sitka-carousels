    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "wordpress",
        "wordpress-plugin"
    ],
    "homepage": "https://bc.libraries.coop/",
    "authors": [
        {
            "name": "Ben Holt",
            "email": "ben.holt@bc.libraries.coop"
        },
        {
            "name": "Jonathan Schatz",
            "email": "jonathan.schatz@bc.libraries.coop"
        },
        {
            "name": "Sam Edwards",
            "email": "sam.edwards@bc.libraries.coop"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "^1.0 || ^2.0",
        "10up/async-transients": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BCLibCoop\\SitkaCarousel\\": "inc/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
